pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    
        stage('develop') {
            steps {
                echo 'stage - DEVELOPMENT'
            }
        }

        stage('deploy') {
            steps {
                echo 'stage - DEPLOYMENT'
            }
        }

        stage('test') {
            steps {
                echo 'stage - TESTING'
            }
        }

        stage('deliver') {
            steps {
                echo 'stage - DELIVER'
            }
        }
    }
}
